		var listOfLocalities = [];
		var localityForDropbox = [];

		var localities = {};//total energy production
		var localitiesEnCon = {};//total energy consumption
		var localitiesCO2 = {};
		var localitiesCoalPro = {};
		var localitiesCoalCon = {};
